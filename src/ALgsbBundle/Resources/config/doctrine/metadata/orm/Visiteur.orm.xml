<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Visiteur" table="visiteur">
    <id name="id" type="string" column="id" length="3">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="prenom" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="login" type="string" column="login" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mdp" type="string" column="mdp" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adresse" type="string" column="adresse" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cp" type="string" column="cp" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ville" type="string" column="ville" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateembauche" type="date" column="dateembauche" nullable="false"/>
  </entity>
</doctrine-mapping>
